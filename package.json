{
  "name": "ig-cli",
  "version": "0.0.2",
  "license": "MIT",
  "bin": "./dist/index.js",
  "scripts": {
    "buy_10_gold": "node dist/index.js --instrument=gold --action=buy --amount=10",
    "close_dax": "node dist/index.js --instrument=dax --action=close",
    "close_dax_live": "node dist/index.js --instrument=dax --action=close --live=true",
    "close_gold": "node dist/index.js --instrument=gold --action=close",
    "close_gold_live": "node dist/index.js --instrument=gold --action=close --live=true",
    "compile": "npx tsc",
    "execute": "node dist/index.js --ships=4",
    "fullbuy_dax": "node dist/index.js --instrument=dax --action=fullbuy",
    "fullbuy_dax_live": "node dist/index.js --instrument=dax --action=fullbuy --live=true",
    "fullbuy_gold": "node dist/index.js --instrument=gold --action=fullbuy",
    "fullbuy_gold_live": "node dist/index.js --instrument=gold --action=fullbuy --live=true",
    "inst": "yarn install",
    "list_dax": "node dist/index.js --instrument=dax",
    "list_dax_live": "node dist/index.js --instrument=dax --live=true",
    "list_gold": "node dist/index.js --instrument=gold"
  },
  "dependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/node": "^13.1.8",
    "axios": "^0.19.1",
    "dotenv": "^8.2.0",
    "ig-api": "^0.15.0",
    "pidcrypt": "nikvdp/pidcrypt",
    "tslint-config-airbnb": "^5.11.2"
  },
  "devDependencies": {
    "@types/ramda": "^0.26.39",
    "@types/yargs": "^15.0.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4",
    "yargs": "^15.1.0"
  }
}